#header-region
.container
  #alerts-region
    - if notice
      = render 'layouts/alert', :alert => notice
  #main-region
  #dialog-region
#footer-region
  = render 'layouts/footer'
= javascript_tag do
  $(function() {
  var current_user_object = {id: "#{ current_user.id}",email: "#{ current_user.email}"};
  var websocket_url = "#{request.host}:#{request.port}/websocket";
  NominaApp.Authorization.start({current_user_object: current_user_object});
  NominaApp.start({ websocket_url: websocket_url });
  });
